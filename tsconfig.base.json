{
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "es2017",
        "module": "ESNext",
        "lib": [
            "ES2020", "DOM"
        ],
        "strict": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}